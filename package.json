        "api",
        "discord",
        "discordapp",
        "Shiro",
        "wrapper"
    ],
    "contributors": [
        "abalabahaha",
        "Suzzudev/shiro"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/SuzzuDev/Shiro.js/issues"
    },
    "dependencies": {
        "ws": "^8.2.3"
    },
    "devDependencies": {
        "@types/node": "^16.11.7",
        "@types/ws": "^8.2.0",
        "@typescript-eslint/eslint-plugin": "^4.33.0",
        "@typescript-eslint/parser": "^4.33.0",
        "eslint": "^7.32.0",
        "eslint-plugin-sort-class-members": "^1.14.0",
        "typescript": "^4.4.4"
    },
    "optionalDependencies": {
        "opusscript": "^0.0.8",
        "tweetnacl": "^1.0.3"
    },
    "browser": {
