        ".": [
            {
                "require": "./index.js",
                "import": "./esm.mjs"
            },
            "./index.js"
        ],
        "./esm": "./esm.mjs"
    },
    "typings": "./index.d.ts",
    "engines": {
        "node": ">=10.4.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SuzzuDev/Shiro.js.git"
    },
